		interrupts = <GIC_SPI 189 IRQ_TYPE_LEVEL_HIGH>;
		clock-frequency = <400000>;
		renesas,clks-per-count = <2>;
                renesas,bus-data-delay = <0>;
                pin_multi = <1>;
               renesas,gpio-scl = <86  510>;
               renesas,gpio-sda = <87  514>;
	       status = "disabled";
	};

